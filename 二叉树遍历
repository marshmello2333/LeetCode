/**
 * Definition for a binary tree node.
 * struct TreeNode {
 *     int val;
 *     TreeNode *left;
 *     TreeNode *right;
 *     TreeNode() : val(0), left(nullptr), right(nullptr) {}//树节点初始化，详细参考结构体初始化
 *     TreeNode(int x) : val(x), left(nullptr), right(nullptr) {}
 *     TreeNode(int x, TreeNode *left, TreeNode *right) : val(x), left(left), right(right) {}
 * };
 */
class Solution {
public:
void inoeder(TreeNode* root,vector<int>&nums){
if(!root)
return;

inoeder(root->left,nums);
nums.push_back(root->val);
inoeder(root->right,nums);


}

    vector<int> inorderTraversal(TreeNode* root) {

//不用进一步判断是否为空，cause inorder已经判断过了
 vector<int>nums;
 inoeder(root,nums);
 return nums;

    }
};
